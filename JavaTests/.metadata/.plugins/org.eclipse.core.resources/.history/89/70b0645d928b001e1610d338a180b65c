package test7;

import java.io.FileOutputStream;

import org.apache.poi.ss.usermodel.Cell;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Sheet;
import org.apache.poi.ss.usermodel.Workbook;
import org.apache.poi.xssf.usermodel.XSSFWorkbook;

public class Q1FlowerNameExcel {

	public static void main(String[] args) {
		// Q1 Flower Name Excel Method 2
		Workbook wb = new XSSFWorkbook();
		Sheet sh = wb.createSheet("Vegitable");
		String[] flowers = {
				"Vegitable1", "Vegitable2", "Vegitable3", "Vegitable4", "Vegitable5",
				"Vegitable6", "Vegitable7", "Vegitable8", "Vegitable9", "Vegitable10",
				"Vegitable11", "Vegitable12", "Vegitable13", "Vegitable14", "Vegitable15",
				"Vegitable16", "Vegitable17", "Vegitable18", "Vegitable19", "Vegitable20"
		};
		for (int i = 1; i < flowers.length; i++) {
			Row row = sh.createRow(i);
			Cell cell = row.createCell(0);
			cell.setCellValue(flowers[i]);
		}
		try (FileOutputStream fileOut = new FileOutputStream("E:\\JavaTests\\AutomationTests\\src\\test7\\Vegitable.xlsx"))
		{
			wb.write(fileOut);
			System.out.println("Excel file has been created successfully!");
		} catch (Exception e)
		{
			e.printStackTrace();
		}
		finally
		{
			try
			{
				wb.close();
			} catch (Exception e)
			{
				e.printStackTrace();
			}
		}
	}
}